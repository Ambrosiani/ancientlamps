[
<% data.catalogue.entries.each_with_index do | entry, i | %>
  {
    "Catalogue/Guide Number": <%= entry.cat_no %>,
    "DOR ID": <%= entry.dor_id %>,
    "Inventory Number": "<%= entry.inv_no %>",
    "Artist/Maker": "",
    "Title": "",
    "Group": "",
    "Location": "<%= entry.place.gsub(/<\/?[^>]*>/, "") unless entry.place.nil? %>",
    "Site": "",
    "Pleiades": "",
    "TGN": "",
    "TGN Web View": "",
    "Begin Date": "<% unless entry.date_numeric.nil? %><%= entry.date_numeric.split("; ")[0] %><% end %>",
    "End Date": "<% unless entry.date_numeric.nil? %><%= entry.date_numeric.split("; ")[1] %><% end %>",
    "Date": "<% unless entry.date.nil? %><%= entry.date.gsub(/<\/?[^>]*>/, "") %><% end %>",
    "Classification": "",
    "Typology": "<% unless entry.type.nil? %><%= entry.type.gsub(/<\/?[^>]*>/, "") %><% end %>",
    "Object Name": "",
    "Medium": "",
    "AAT": "",
    "AAT Web View": "",
    "Munsell": "",
    "Height (cm)": "",
    "Width (cm)": "",
    "Depth (cm)": "",
    "Length (cm)": "",
    "Diameter (cm)": "",
    "Dimensions": "<%= entry.dimensions %>",
    "Object Description": "<% unless entry.description.nil? %><%= entry.description.gsub(/<\/?[^>]*>/, "").gsub(/\s{2,}/, " ") %><% end %>",
    "Culture": "",
    "Image URL": "http://www.getty.edu/publications/ancientlamps/assets/images/thumbs/<%= entry.cat_no %>.jpg",
    "Collection Page URL": "<% unless entry.dor_id.nil? %>http://www.getty.edu/art/collection/objects/<%= entry.dor_id %><% end %>"
  }<%= ',' if i < (data.catalogue.entries.size - 1) %>
<% end %>
]
